{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "distribution": "store"
    },
    "android-apk": {
      "android": {
        "buildType": "apk"
      },
      "distribution": "internal",
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccount": "keystore.json",
        "track": "internal"
      }
    }
  }
}
